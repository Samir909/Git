@model System.Data.DataTable
@{
    ViewBag.Title = "Index";
}

<h2>view all</h2>

<table class="table table-bordered table-striped">
    <tr>
        <th>Admin Id</th>
        <th>Admin Full Name</th>
        <th>Admin User Name</th>
        <th>Admin Password</th>
        <th></th>
        <th></th>
    </tr>


    @for (int i = 0; i < Model.Rows.Count; i++)
    {
<tr>
    <td>@Model.Rows[i][0]</td>
    <td>@Model.Rows[i][1]</td>
    <td>@Model.Rows[i][2]</td>
    <td>@Model.Rows[i][3]</td>
    <td>
        <a href="@Url.Action("Edit","Admin",new {@ID=@Model.Rows[i][0]})">Edit</a>
    </td>

    <td>
        <a href="@Url.Action("Delete","Admin",new {@ID=@Model.Rows[i][0]})">Delete</a>
    </td>

</tr>


    }

</table>

<a href="@Url.Action("Create","Admin")">Add Admin</a>